  background-clip: border;
  border-image: none;
  border:1px solid palette(dark);
  max-width: 200 px;
  outline: 0;
}

dbgdlg > QLabel
{
  max-height : 27 px;
}

dbgdlg > QPushButton
{
  max-width : 100 px;
  max-height : 25 px;
}
 
PythonQtScriptingConsole
{
  color: #a1a1a1;
