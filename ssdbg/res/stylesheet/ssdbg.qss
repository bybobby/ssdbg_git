QToolTip{
  color:#ffffff;
  background-color:palette(base);
  border:1px solid palette(highlight);
  border-radius:4px;
}

ssdbg {
  border:1px solid palette(highlight); 
  border-radius:0px 0px 0px 0px; 
  background-color:rgb(33,42,60);
  visible : true;
}


ssdbg::active {
  border:1px solid palette(highlight); 
  border-radius:0px 0px 0px 0px; 
  background-color:rgb(33,42,60);
}

ssdbg::!active {
  border:1px solid #000000;
  border-radius:0px 0px 0px 0px;
  background-color:rgb(33,42,60);
}

QToolButton[objectName^="minimize"]
{
  background-color:none;
  border:none;
  width:16px;
  height:16px;
  padding:4px;
  image:url(:/res/img/icon_window_minimize.png); 
} 

QToolButton#close
{
  background-color:none;
  border:none;
  width:16px;
  height:16px;
  padding:4px;
  image:url(:/res/img/icon_window_close.png);
  border-top-right-radius: 5px;
} 

QLineEdit[objectName^="medit"]
{
  max-width: 45 px;
  min-height: 25 px;
}

QTabBar::tab 
{ 
  height: 35px;
  width: 120px;
}

QTabWidget::tab-bar:top
{
  left : 80 px;
}

QTabWidget::tab-bar:left 
{
  right : 20 px;
}

QTabWidget::tab-bar:right 
{
  left : 20 px; 
}

gphdlg
{
  color: #b1b1b1;
  background-color: #212a3c;
  selection-background-color:#212a3c;
  selection-color: black;
  background-clip: border;
  border-image: none;
  border: 0px transparent black;
  outline: 0;
}

imgdlg
{
  color: #b1b1b1;
  background-color: #212a3c;
  selection-background-color:#212a3c;
  selection-color: black;
  background-clip: border;
  border-image: none;
  border: 0px transparent black;
  outline: 0;
}

dbgdlg
{
  color: #b1b1b1;
  background-color: #212a3c;
  selection-background-color:#212a3c;
  selection-color: black;
  background-clip: border;
  border-image: none;
  border: 0px transparent black;
  outline: 0;
}

dbgdlg > QTextEdit[objectName^="log"]
{
  color: #a1a1a1;
  background-color: #000000;
  selection-background-color:#000000;
  selection-color: black;
  background-clip: border;
  border-image: none;
  border:1px solid palette(dark);
  outline: 0;
}

dbgdlg > QListWidget[objectName^="flist"]
{
  color: #a1a1a1;
